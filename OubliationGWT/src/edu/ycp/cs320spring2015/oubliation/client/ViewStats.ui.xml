<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.screen {
			max-width: 1000px;
		}
		
		.center {
			text-align: center;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.screen}">
		<g:FlowPanel ui:field="statContainer">
		</g:FlowPanel>
		<g:Hyperlink styleName="{style.center}" ui:field="backLink">Back</g:Hyperlink>
	</g:FlowPanel>
</ui:UiBinder> 